<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.shimsoon.today_i_dressedup">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">

        <meta-data
                android:name="com.google.android.gms.ads.APPLICATION_ID"
                android:value="ca-app-pub-7264194476227331~5762708126"/>

        <service
                android:name="com.shimsoon.today_i_dressedup.data.firebase.MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

        <activity android:name="com.shimsoon.today_i_dressedup.ui.postDetail.PostDetailActivity"
                  android:parentActivityName="com.shimsoon.today_i_dressedup.ui.postList.PostActivity">
        </activity>
        <activity android:name="com.shimsoon.today_i_dressedup.ui.postList.PostActivity"
                  android:parentActivityName="com.shimsoon.today_i_dressedup.ui.myPage.MyPageActivity">
        </activity>
        <activity android:name="com.shimsoon.today_i_dressedup.ui.auth.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.shimsoon.today_i_dressedup.ui.auth.SignUpActivity">
        </activity>
        <activity android:name="com.shimsoon.today_i_dressedup.ui.myPage.MyPageActivity"
                  android:parentActivityName="com.shimsoon.today_i_dressedup.ui.evaluate.EvaluateActivity">
        </activity>
        <activity android:name="com.shimsoon.today_i_dressedup.ui.evaluate.EvaluateActivity">
        </activity>
    </application>

</manifest>